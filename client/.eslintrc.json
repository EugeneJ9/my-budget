{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    // temporary turn off console.log warnings, to have some vision in console
    // "no-console": "warn",
    "semi": "warn",
    "no-unused-vars": "warn"
  }
}
